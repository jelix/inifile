{
    "name": "jelix/inifile",
    "type": "library",
    "description": "classes to read and modify ini files by preserving comments and empty lines",
    "keywords": ["ini", "files"],
    "homepage": "https://github.com/jelix/inifile",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        },
        {
            "name": "Loic Mathaud"
        },
        {
            "name": "Riccardo Mazzei"
        }
    ],
    "require": {
        "php": ">=5.6.1"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.8.*"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4" : {
            "Jelix\\IniFile\\" : "lib/"
        }
    },
    "minimum-stability": "stable"
}
